<body>
    <div class="" id="">
        <section id="Icsi_top_header">
            <div class="container-fluid">
                <div class="Header">
                    <div class="LogoSpacing">
                        <div class="header-part">
                            <div class="">
                                <div class="col-md-7">
                                    <a href="https://www.icsi.edu/home">
                                        <img src="~/Content/Images/logo_full.png" alt="logo" class="img-responsive " id="mainLogo">
                                    </a>
                                </div>
                                <div class="col-md-5">
                                    <div class="helplineNum">
                                        <p><span>Helpline Number</span>: 0120 - 4522000</p>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </section>
        <section>
            <div class="navbar navbar-default nav-color">
                <div class="contain-nav-all">
                    <div class="navbar-header">
                        <a class="navbar-brand" href="#"></a>
                        <a class="navbar-toggle toggleBorder" data-toggle="collapse" data-target=".navbar-collapse">
                            <span class="icon-bar iconColor"></span>
                            <span class="icon-bar iconColor"></span>
                            <span class="icon-bar iconColor"></span>
                        </a>
                    </div>
                    @*<div class="navbar-collapse collapse">
                        <ul class="nav navbar-nav navSide">
                            <li class="dropdown">
                                <a class="dropdown-toggle" data-toggle="dropdown" href="#" aria-expanded="false">Company / PCS Registration <span class="caret"></span></a>
                                <ul class="dropdown-menu drpdown-hdr-menu">
                                    <li><a href="Company_Registration.html">New Company Registration</a></li>
                                    <li><a href="Company_pcs_Registration.html">New PCS Registration</a></li>
                                </ul>
                            </li>
                        </ul>
                    </div>*@
                </div>
            </div>
        </section>
