<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - ICSI Application</title>

    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.4.0/css/bootstrap.min.css">
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.4.1/jquery.min.js"></script>
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.4.0/js/bootstrap.min.js"></script>
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">
    <script src="https://unpkg.com/gijgo@1.9.13/js/gijgo.min.js" type="text/javascript"></script>
    <link href="https://unpkg.com/gijgo@1.9.13/css/gijgo.min.css" rel="stylesheet" type="text/css" />
    <link rel="stylesheet" href="~/Content/CSR/CSS/style.css" />

    <script type="text/javascript" src="~/Scripts/md5.js"></script>

</head>


<body>
    <div class="" id="">
        <div class="contain-all">
            <div class="row">
                <div class="col-xs-12 col-sm-4">
                    <a href="https://www.icsi.edu/home"><img src="~/Content/CSR/Images/logo_full.png" alt="logo" style="padding:8px;" class="img-responsive"> </a>
                </div>
                <div class="col-xs-12 col-sm-8">
                    <div class="helplineNum">
                        <p id="pHelpline"><span>For any Query</span>: csrawards@icsi.edu</p>
                    </div>
                </div>
            </div>
        </div>

        <section class="navbar navbar-default nav-color">
            <div class="contain-all">
                <div class="navbar-header">
                    <button type="button" class="navbar-toggle" data-toggle="collapse" data-target="#myNavbar">
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                    </button>
                    <a class="navbar-brand hidden-md hidden-lg" href="#">Select Menu</a>
                </div>
                <div class="collapse navbar-collapse" id="myNavbar">
                    <ul class="nav navbar-nav">
                        <li><a href="../Home/RO_Login"> Home </a></li>
                    </ul>
                </div>
            </div>
        </section>

    </div>

    @RenderBody()

    <section class="icsiFooter">
        <footer>
            <div class="container no-padding">
                <div class="footerContainer">
                    <div class="no-padding col-lg-12 text-center">
                        <ul class="list-inline icsiFooterMenu">
                            <li><a href="https://www.icsi.edu/JournalsBulletins.aspx" target="_blank">Journals &amp; Bulletins</a></li>
                            <li><a href="https://www.icsi.edu/CompanyLaw.aspx" target="_blank">Regulatory Updates</a></li>
                            <li><a href="https://www.icsi.edu/AboutUs/CCGRTResearchInitiative.aspx" target="_blank">ICSI-CCGRT</a></li>
                            <li><a href="https://www.icsi.edu/helpdesk.aspx" target="_blank">Help Desk</a></li>
                            <li><a href="https://www.icsi.edu/MediaCentre.aspx" target="_blank">TV &amp; Radio Spots on CS</a></li>
                            <li><a href="https://www.icsi.edu/terms.aspx" target="_blank">Terms &amp; Conditions</a></li>
                            <li><a href="https://www.icsi.edu/privacy.aspx" target="_blank">Privacy Policy</a></li>
                            <li><a href="https://www.icsi.edu/ContactUs.aspx" target="_blank">Contact Us</a></li>

                        </ul>
                    </div>
                    <div class="col-lg-12 no-padding text-center">
                        <p>
                            Best viewed on screen resolution 1024x768 pixels<br> Website best viewed in Mozila 38.0 and above, Chrome 39.0
                        </p>
                    </div>
                    <div class="col-lg-12 no-padding">
                        <div class="col-lg-8 no-padding">
                            <p>
                                Copyright © 2019. All Rights Reserved. The Institute of Company Secretaries of India
                            </p>
                        </div>
                        <div class="col-lg-4 no-padding">
                            <p class="pull-right powerby">
                                <span class="PoweredBySpan">Powered by:</span>
                                <a href="http://smash.icsi.in/Scripts/login.aspx" target="_blank">
                                    <img src="~/Content/CSR/Images/icsi_icon.png" alt="logo">
                                </a>
                            </p>
                        </div>
                    </div>
                </div>
            </div>
        </footer>
    </section>

    <script>

        function screenAction(si, uq, uniqueregID) {
            var frm = document.createElement('form');
            frm.method = "post";
            frm.action = "../Pcs/pcs";
            var u = 'pcscomp';
            frm.appendChild(createInputHidden('u', u));
            frm.appendChild(createInputHidden('si', si));
            if (uq)
                frm.appendChild(createInputHidden('ui', uq));
            frm.appendChild(createInputHidden('s', uq ? 'edit' : 'new'));
            if (uniqueregID)
                frm.appendChild(createInputHidden('UNIQUE_REG_ID', uniqueregID));
            document.body.appendChild(frm);
            frm.submit();
        }

        function createInputHidden(nm, vl) {
            var hi = document.createElement('input');
            hi.type = 'hidden';
            hi.name = nm;
            hi.value = vl;
            return hi;
        }
        function loadRecord(uq, si) {
            var u = document.getElementById('u').value;
            var frm = document.createElement('form');
            frm.method = "post";
            frm.action = "../PCS/PCS";
            frm.appendChild(createInputHidden('u', u));
            if (si) {
                frm.appendChild(createInputHidden('si', si));
            }
            frm.appendChild(createInputHidden('ui', uq));
            frm.appendChild(createInputHidden('s', 'edit'));
            if (document.cookie.length > 0) {
                frm.appendChild(createInputHidden('uniqueId', document.cookie.split(';')[0]));
            }
            document.body.appendChild(frm);
            frm.submit();
        }

        function loadAnonymous() {
            //document.forms[0].hidden = true;
            $(".home-login-box").attr("style", "visibility:hidden");
            document.getElementById('email').value = 'complaintmem';
            document.getElementById('password').value = 'password';
            document.getElementById('btnSubmit').click();

        }
    </script>
</body>
</html>

