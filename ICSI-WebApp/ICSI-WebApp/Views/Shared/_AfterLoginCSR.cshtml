        function loadPaginationNextPage() {
            var newpgn = parseInt($("[name=pren]").val()) + 1;
            if (newpgn > parseInt($("[name=totpgs]").val())) {
                alert("Next page not Available");
            } else processPagenation(newpgn);
        }

        function loadPaginationPage(nm) {
            var newpgn = 0;
            var pren = parseInt($("[name=pren]").val());
            var totalpgs = parseInt($("[name=totpgs]").val());
            var pp = Math.floor(pren / 5);
            var ppp = pp * 5;
            newpgn = ppp + nm;
            if (pp > totalpgs) {
                alert("No Data available for this page");
            } else processPagenation(newpgn);
        }

        function loadPaginationPrevPage() {
            var newpgn = parseInt($("[name=pren]").val()) - 1;
            if (newpgn < 1) {
                alert("Previous page not Available");
            } else processPagenation(newpgn);
        }

        function loadPaginationOrigPage(tp) {
            var newpgn = 0;
            if (tp == 1) {
                newpgn = parseInt($("#paginationid").val());
            } else {
                newpgn = parseInt($("#paginationdownid").val());
            }
            if (newpgn < 1 || newpgn > parseInt($("[name=totpgs]").val())) {
                alert("Invalid page number");
            } else processPagenation(newpgn);

        }

        function processPagenation(newpgn) {
            var formdatas = new FormData();
            formdatas.append("u", $("[name=u]").val());
            formdatas.append("s", "search");
            formdatas.append("m", $("[name=m]").val());
            if ($("[name=screenid]").length > 0) formdatas.append("si", $("[name=screenid]").val());
            else if ($("[name=si]").length > 0) formdatas.append("si", $("[name=si]").val());
            formdatas.append("pgr", $("[name=pgr]").val());
            formdatas.append("pgn", newpgn);
            //field.name!='undefined' &&
            var fields = $(":input").serializeArray();
            jQuery.each(fields, function (i, field) {
                if (field.name.startsWith("SCRHPGN_")) {
                    formdatas.append(field.name.replace("SCRHPGN_", "SCRH_"), field.value);
                } else if (field.name.startsWith("CONDPGN_")) {
                    formdatas.append(field.name.replace("CONDPGN_", "COND_"), field.value);
                } else if (field.name.startsWith("REPLACEPGN_")) {
                    formdatas.append(field.name.replace("REPLACEPGN_", "REPLACE_"), field.value);
                } else if (field.name.startsWith("MANSCRPGN_")) {
                    formdatas.append(field.name.replace("MANSCRPGN_", "MANSCR_"), field.value);
                }
            });
            $.ajax({
                url: 'AjaxPagenationSearch',
                dataType: 'json',
                cache: false,
                contentType: false,
                processData: false,
                data: formdatas,
                type: 'post',
                success: function (response) {
                    if (response.resCode == 0 && response.htmlstring && response.htmlstring != "") {
                        $("#pagesearchtbid").empty();
                        $("#pagesearchtbid").html(response.htmlstring);
                        $("#paginationid").val(newpgn);
                        $("#paginationdownid").val(newpgn);
                        $("[name=pren]").val(newpgn);
                        $('#ulpgid').find('li').removeAttr('class');
                        $('#ulpgdnid').find('li').removeAttr('class');
                        var pgr = parseInt($("[name=pgr]").val());
                        var totalpgs = parseInt($("[name=totpgs]").val());
                        var pp = Math.trunc(newpgn / 5);
                        if (pp != 0 && pp % 5 == 0) pp = pp - 1;
                        var ppp = pp * 5;
                        var kpp = newpgn - ppp;
                        for (var dp = 1; dp < 6; dp++) {
                            var pppp = ppp + dp;
                            $("#pgli" + dp).find('a').html(pppp);
                            $("#pgdnli" + dp).find('a').html(pppp);
                            if (kpp == dp) {
                                $("#pgli" + dp).addClass('active');
                                $("#pgli" + dp).addClass('disable');
                                $("#pgdnli" + dp).addClass('active');
                                $("#pgdnli" + dp).addClass('disable');
                            }
                        }
                    }
                    else { alert("No Records Found"); }
                }
            });
        }

