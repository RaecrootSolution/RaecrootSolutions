        function uploadSTDocumentinfolder(foldername, doctypeid, doctypename) {
            formdata = new FormData();
            if ($('#commonuploadfileintofolder').prop('files').length > 0) {
                file = $('#commonuploadfileintofolder').prop('files')[0];
                formdata.append('file', file);
            }
            var trid = "";
            if ($("#tblUploaded tr").length == 1) {
                trid = "tr1";
            }
            else {
                var previd = $("#tblUploaded tr:last").attr("id");
                previd = previd.replace("tr", "");
                trid = "tr" + parseInt(previd) + 1;
            }
            //var trid = "";
            //trid = "tr" + $("#tblUploaded tr").length;
            formdata.append("Folder_Name", foldername);
            formdata.append("doctype_id", doctypeid);
            formdata.append("doctype_name", doctypename);
            formdata.append("trid", trid);
            Loader();
            $.ajax({
                url: 'UploadFilesinFolder',
                dataType: 'json',
                cache: false,
                contentType: false,
                processData: false,
                data: formdata,
                type: 'post',
                success: function (response) {
                    if (response.resCode == 1) {
                        var doctype = response.doctypename;;
                        var doctype_id = response.doctypeid;
                        var result = response.htmlstring;
                        var res = result.split("~");
                        var filename = "";
                        var fullpath = "";
                        if (res.length > 0) {
                            filename = res[0];
                            fullpath = res[1];
                        }
                        var htmlstring = "";
                        //htmlstring += "<table class='table table-bordered tableSearchResults'><thead><tr><th class='searchResultsHeading'>Document Type</th><th class='searchResultsHeading'>Download</th><th class='searchResultsHeading'>Remove</th></tr></thead >";
                        htmlstring += "<tr id=" + response.trid + "><td>" + doctype + "</td>";
                        htmlstring += "<td><input type='hidden' value='" + doctype_id + "' id='h_doctypeid' /><input type='hidden' value='" + fullpath + "' id='h_path' class='downloaddoc' /><input type='hidden'  value='" + filename + "' id='h_file' class='downloadfile' /><button type='button' class='btn btn - primary btn - xs' onclick=DocDownloadintoFolder(this)>Download</button></td>";
                        htmlstring += "<td><input type='hidden' value='" + fullpath + "' class='removedoc' /><button type='button' class='btn btn - primary btn - xs' onclick=DocRemovefromFolder(this)>Remove</button></td></tr>";
                        jQuery("#tblUploaded tbody").append(htmlstring);
                        // $("#UploadedDoc_div").append(htmlstring);
                    }
                    else { alert(response.resMessage); }
                }
            });
            $("#DOC_TYPE_ID").val("");
            $("#commonuploadfileintofolder").replaceWith($("#commonuploadfileintofolder").val('').clone(true));
            RemoveLoader();
        }
