                $(this).attr("disabled", "disabled");
            });

            var obj = {};
            //Find all input elements
            var panel = $("#" + divId);
            var inpts = panel.find("input,textarea,select");
            if (inpts.length > 0) {
                $.each(inpts, function (key, value) {
                    if (value.type === 'text' ||
                        value.type === 'email' ||
                        value.type === 'url' ||
                        value.type === 'number' ||
                        value.type === 'file' ||
                        //value.type === 'hidden' ||
                        value.type === 'password' ||
                        value.type === 'textarea' ||
                        value.type === 'select' ||
                        value.type === 'date' ||
                        value.type === 'select-one') {
                        $(this).attr("disabled", "disabled");
                    }
                    else if (value.type === 'checkbox' || value.type === 'radio') {
                        $(this).attr("disabled", "disabled");
                    }
                });
            }
        }

        function maxlength(element) {
            var minvalue = 150;
            var maxvalue = 300;
            var value = $(element).val();
            var words = value.split(' ');

            if (words.length > maxvalue) {
                alert("Please enter the words between 150 to 300.");
            }
        }

        if ($("#user_type_id").val() == "14") {
            $("#frm-steps ul li").eq(0).hide();
        }

        function isDNumberKey(evt) {
            var charCode = (evt.which) ? evt.which : event.keyCode
            if (charCode != 46) {
                if (charCode > 31 && (charCode < 48 || charCode > 57)) {
                    alert('Please enter only digit.');
                    return false;
                }
            }
            return true;
        }

        function ValidateSize(file, QuesId) {
            var FileSize = file.files[0].size / 1024 / 1024; // in MB
            if (FileSize > 4) {
                alert('The file size can not exceed 4 MB.');
                $(file).val('');
            }

            $("#" + QuesId + "_lblFile").attr("style", "font-weight:normal; font-size:12px;color:green;").html("File has been attached.");
            var UploadedFile = $("#UPLOADED_FILE").val();
            $("#UPLOADED_FILE").val(parseInt(UploadedFile) + 1);
        }

        if ($("#user_type_id").val() == "14") {
            $(".helplineNum p").html("");
            $(".helplineNum p").attr("style", "padding-top: 30px;");
        }
    </script>
</body>
</html>